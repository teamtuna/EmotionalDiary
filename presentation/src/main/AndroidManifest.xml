<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamtuna.emotionaldiary.presentation">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EmotionalDiary">
        <activity
            android:name="com.teamtuna.emotionaldiary.main.MainActivity"
            android:theme="@style/Theme.EmotionalDiary.Splash">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.teamtuna.emotionaldiary.history.HistoryActivity" />

    </application>

</manifest>